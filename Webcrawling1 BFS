#include <iostream>
using namespace std;

#define MAX 10

int main() {
    int n; // number of web pages
    cout << "Enter number of web pages: ";
    cin >> n;

    int adj[MAX][MAX];  // adjacency matrix
    cout << "Enter adjacency matrix (1 = link exists, 0 = no link):\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> adj[i][j];
        }
    }

    int visited[MAX] = {0};
    int queue[MAX];
    int front = 0, rear = -1;

    int start;
    cout << "Enter starting page (0-" << n - 1 << "): ";
    cin >> start;

    cout << "\nIndexing order (BFS): ";

    // Start BFS
    queue[++rear] = start;
    visited[start] = 1;

    while (front <= rear) {
        int page = queue[front++];
        cout << "Page" << page << " ";

        for (int i = 0; i < n; i++) {
            if (adj[page][i] == 1 && !visited[i]) {
                queue[++rear] = i;
                visited[i] = 1;
            }
        }
    }

    cout << endl;
    return 0;
}
